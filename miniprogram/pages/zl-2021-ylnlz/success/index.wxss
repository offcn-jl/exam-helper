.con{
  width: 670rpx;
  margin: 50rpx auto;
  margin-top: 800rpx;
}
.con .pic{
  float: left;
  background:  url(http://jl.offcn.com/zg/ty/images/exam-helper/zl-2021-ylnlz/list.png) no-repeat top;
  background-size: 100% 100%;
  width: 126rpx;
  height: 129rpx;
  margin: 20rpx 5rpx;
}
.con .pic view{
  font-size: 18rpx;
  text-align: center;
  color:#fff;
  margin-top: 100rpx;
}
.con .pic .t2{
  font-size: 15rpx;
  line-height: 30rpx;
}
.popup{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
.popup text{
  display: table;
  margin: 50vh auto;
  background: #fff;
  border-radius: 20rpx;
  padding: 40rpx;
  text-align: center;
}