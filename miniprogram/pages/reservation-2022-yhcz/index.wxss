.main1{
  width: 100vw;
  height: 409rpx;
  background: url(http://jl.offcn.com/zt/ty/2021images/exam-helper/22yhcz-main1.jpg) no-repeat top;
  background-size: 100% 100%;
  overflow: hidden;
}
.main1 view{
  font-size: 40rpx;
  text-align: center;
  color: #fff;
  font-weight: bold;
  margin: 20rpx auto;
  margin-top: 50rpx;
}
.main1 image{
  display: block;
  margin: 0 auto;
  width: 725rpx;
}
.main2{
  width: 100vw;
  height: 527rpx;
  background: url(http://jl.offcn.com/zt/ty/2021images/exam-helper/22yhcz-main2.jpg) no-repeat top;
  background-size: 100% 100%;
  overflow: hidden;
}
.main2 .m2_top{
  width: 96vw;
  height: 230rpx;
  background: url(http://jl.offcn.com/zt/ty/2021images/exam-helper/22yhcz-m2.png) no-repeat top;
  background-size: 100% 100%;
  overflow: hidden;
  margin: 30rpx auto;
}
.main2 .m2_top view{
  color: #aae3fe;
  font-size: 22rpx;
  margin-left: 67rpx;
  margin-top: 17rpx;
}
.main2 .m2_top text{
  font-size: 19rpx;
  line-height: 29rpx;
  width: 90%;
  display: block;
  margin: 10rpx;
  text-indent: 40rpx;
}
.main2 .m2_bot{
  width: 96vw;
  margin: 0 auto;
}
.main2 .m2_bot view{
  color: #aae3fe;
  font-size: 25rpx;
  width: 96%;
  border: 3rpx solid #aae3fe;
  padding: 2%;
}
.main2 .m2_bot image{
  width: 690rpx;
  height: 111rpx;
  margin: 30rpx auto;
  display: block;
}
.main3 {
  margin-bottom: 120rpx;
}
.main3 .m3_tit{
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
}
.main3 .list{
  width: 690rpx;
  height: 112rpx;
  background: url(http://jl.offcn.com/zt/ty/2021images/exam-helper/22yhcz-m3.png) no-repeat top;
  background-size: 100% 100%;
  margin: 20rpx auto;
}
.main3 .list .list_tit{
  font-size: 20rpx;
  font-weight: bold;
  color: #ff0000;
  margin-left: 12rpx;
}
.main3 .list text{
  font-size: 18rpx;
  display: block;
  margin-left: 12rpx;
  width: 94%;
}
.main3 .list text view{
  color: #ff0000;
}
.popup{
  width: 100vw;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2;
  background: url(http://jl.offcn.com/zt/ty/2021images/exam-helper/22yhcz-main1.jpg) no-repeat top;
  background-size: 100% ;
}
.btn {
  background: #FF9800;
  color: #fff;
  font-size: 30rpx;
  line-height: 60rpx;
  width: 60vw;
  margin: 20rpx auto;
}