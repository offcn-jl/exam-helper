page{
  background: #ffffff;
  padding-bottom: 50rpx;
}

.banner_bk{
  width:750rpx;
  height:596rpx;
  position: absolute;
  left:0;
  top:0;
  z-index:-1;
}
.container{
  width:750rpx;
  position:relative;
  z-index:1;
}
.nav_title{
  padding-bottom:40rpx;
}
.nav_title image{
  width:586rpx;
  height:118rpx;
  display:block;
  margin:0 auto;
  padding-top:50rpx;
}
.select_wrap{
  width:640rpx;
  background: #fff;
  border-radius:15rpx;
  margin:0 auto;
  padding-top:60rpx;
  padding-bottom: 50rpx;
  -moz-box-shadow:0px 0px 12rpx rgba(0,0,0,0.1); 
  -webkit-box-shadow:0px 0px 12rpx rgba(0,0,0,0.1); 
  box-shadow:0px 0px 12rpx rgba(0,0,0,0.1);
}
.offer_item{
  width:550rpx;
  height:90rpx;
  margin:0 auto 40rpx;
}
.offer_item.offer_itemx{
  background: #f5f3f3;
  border-radius:20rpx;
  line-height: 90rpx;
  display:-webkit-flex;
  justify-content: flex-start;
  border:1px solid #fff;
}
.offer_item.offer_itemx.active{
  border:1px solid #d32002;
  background-color: #fff;
}
.offer_item.offer_itemx.active .iconfont{
  color: #d32002;
}
.offer_item.offer_itemx.active input{
  color: #d32002;
}
.offer_item.offer_itemx .iconfont{
  display: block;
  font-size: 40rpx;
  padding-left:20rpx;
  color: #a3a3a3;
  font-weight: bold;
}
.offer_item.offer_itemx .zysearch{
  display:block;
  font-size: 40rpx;
  padding-left: 20rpx;
  color: #a3a3a3;
  width:490rpx;
  height:90rpx;
  line-height:90rpx;
  font-size:30rpx;
}
.phcolor{
  color: #a3a3a3;
  font-size:30rpx;
}
.tip_show{
  width:550rpx;
  margin:0 auto;
}
.tip_show .tip_title{
  line-height:38rpx;
  font-size:28rpx;
  color: #d32002;
  font-weight:bold;
}
.tip_show .tip_title .iconfont{
  margin-right:5rpx;
  font-size:26rpx;
  position: relative;
  top:2rpx;
}
.tip_detail{
  text-indent:2em;
  font-size:24rpx;
  line-height:38rpx;
  margin-top:10rpx;
}
.tip_detail text{
  color: #d32002;
}
.result_error{
  width:640rpx;
  height:35rpx;
  margin:0 auto;
  line-height:35rpx;
  font-size:30rpx;
  text-align:center;
  color: #d32423;
  padding-top:40rpx;
}





.result_wrap{
  width:750rpx;
  height:227rpx;
  background:url(https://tk.zgjsks.com/sa/skxcx/web/zy_result_bk.png) no-repeat center center;
  background-size:750rpx 227rpx;
  margin-top:40rpx;
}
.result_title{
  text-align: center;
  line-height:43rpx;
  font-size:24rpx;
  color: #fff;
  letter-spacing:20rpx;
}

.result_ok{
  width:627rpx;
  height:143rpx;
  margin:0 auto;
  position:relative;
}
.result_ok .text1{
  display: block;
  color: #333;
  font-size:30rpx;
  line-height:30rpx;
  padding-top:30rpx;
  padding-left:50rpx;
}
.result_ok .text2{
  display: block;
  color: #d32002;
  font-size:36rpx;
  line-height:40rpx;
  font-weight:bold;
  padding-left:50rpx;
  padding-top:10rpx;
}
.result_ok .copy_btn{
  width:125rpx;
  height:47rpx;
  border:1px solid #d32002;
  border-radius:26rpx;
  background: #f57964;
  position: absolute;
  top:47rpx;
  left:465rpx;
  color: #fff;
  font-size:24rpx;
  line-height:47rpx;
  text-align: center;
}
.zy_pic{
  width:650rpx;
  height:111rpx;
  display: block;
  margin:0 auto;
}





.search_btn{
  width:560rpx;
  height:82rpx;
  padding:0;
  background: #d32002;
  line-height:82rpx;
  color: #fff;
  font-size:36rpx;
}
.tip_cvover{
  width:100%;
  position: fixed;
  bottom:20rpx;
  left: 0;
  border-radius:10rpx 10rpx 0 0;
  padding-top:1rpx;
}
.tip_cvover .tip_con{
  width:700rpx;
  height:92rpx;
  background: #d32423;
  margin:0 auto;
  border-radius:15rpx;
  display:flex;
  display: -webkit-flex;
  justify-content: flex-start;
}
.tip_cvover .tip_con .close_tip{
  width:39rpx;
  height:39rpx;
  padding:26.5rpx;
  margin-left:24rpx;
}
.tip_cvover .tip_con .tip_text{
  width:469rpx;
  height:92rpx;
  font-size:32rpx;
  color: #fff;
  text-align: center;
  line-height:92rpx;
}
.tip_cvover .tip_con .tip_icon{
  width:22rpx;
  height:38rpx;
  padding:26.5rpx;
}

.result_titles{
  width:640rpx;
  height:35rpx;
  margin:0 auto;
  line-height:35rpx;
  font-size:30rpx;
  text-align:left;
  color: #d32423;
  padding-top:40rpx;
} 
.result_titles .iconfont{
  margin-right:5px;
}



.result_list{
  padding-top:20rpx;
}
.result_item{
  width:560rpx;
  margin:0 auto;
  background: #fff;
  border-radius:15rpx;
  -moz-box-shadow:0rpx 0rpx 7rpx #d6d7d3;
  -webkit-box-shadow:0rpx 0rpx 7rpx #d6d7d3;
  box-shadow:0rpx 0rpx 7rpx #d6d7d3;
  margin-bottom:35rpx;
  padding:40rpx 40rpx 80rpx 40rpx;
  overflow:hidden;
  position: relative;
}

.result_code{
  display: block;
  font-size:24rpx;
  color: #3b444e;
  line-height:30rpx;
  padding-left:10rpx;
  padding:10rpx 0;
}
.result_detail{
  display:block;
  font-size:30rpx;
  color: #999999;
  line-height:40rpx;
  background: #f4f8f9;
  padding:10rpx;
  padding-left:10rpx;
  border-radius:10rpx;
  margin:20rpx 0 ;
}
.add_btn{
  width:196rpx;
  height:54rpx;
  background: #d32423;
  line-height:54rpx;
  position:absolute;
  left:50%;
  margin-left:-98rpx;
  bottom:20rpx;
  padding:0;
  text-align: center;
  color: #fff;
  border-radius:27rpx;
  font-size:29rpx;
}
.add_btn::after{
  border:none;
}

.single-page {
  font-size: 20px;
  text-align: center;
  line-height: 100px;
  color: orange;
}

.suffix {
  opacity: 0.9;
  position: absolute;
  left: 43vw;
  font-size: 14px;
  color: white;
}

